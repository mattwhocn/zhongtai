#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/ts-node-esm@0.0.6_typescript@5.7.3/node_modules/ts-node-esm/bin/node_modules:/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/ts-node-esm@0.0.6_typescript@5.7.3/node_modules/ts-node-esm/node_modules:/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/ts-node-esm@0.0.6_typescript@5.7.3/node_modules:/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/ts-node-esm@0.0.6_typescript@5.7.3/node_modules/ts-node-esm/bin/node_modules:/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/ts-node-esm@0.0.6_typescript@5.7.3/node_modules/ts-node-esm/node_modules:/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/ts-node-esm@0.0.6_typescript@5.7.3/node_modules:/Users/zhangmeng/Documents/Works/zhongtai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node-esm/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../ts-node-esm/bin/cli.mjs" "$@"
fi
